			<script id="tmp-brewery-search" type="text/template">
				&lt;ul class="brewery-results"&gt;
					&lt;% if (results.length) { %&gt;
						&lt;% _.each(results, function(v) { %&gt;
							&lt;li&gt;
								&lt;a href="/brewery/&lt;%=v.brewery.brewery_id%&gt;"&gt;
								&lt;% if (v.brewery.brewery_label.indexOf('badge-brewery-default') &lt; 0) { %&gt;
									&lt;img src="&lt;%=v.brewery.brewery_label%&gt;" /&gt;
								&lt;% } else { %&gt;
									&lt;img src="images/default-label.png" /&gt;
								&lt;% } %&gt;
								&lt;%= v.brewery.brewery_name %&gt;
								&lt;/a&gt;
							&lt;/li&gt;
						&lt;% }); %&gt;
					&lt;% } else { %&gt;
						&lt;li&gt;No breweries found.&lt;/li&gt;
					&lt;% } %&gt;
				&lt;/ul&gt;
			</script>

			<script id="tmp-officials" type="text/template">
				&lt;% _.each(data, function(v) { %&gt;
					&lt;% if (v.length) { %&gt;
						&lt;optgroup label="&lt;%=v[0].state%&gt;"&gt;
							&lt;% _.each(v, function(x) { %&gt;
								&lt;option value="&lt;%= x.slug %&gt;"&gt;&lt;%= x.label %&gt;&lt;/option&gt;
							&lt;% }); %&gt;
						&lt;/optgroup&gt;
					&lt;% } %&gt;
				&lt;% }); %&gt;
			</script>

