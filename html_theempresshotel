			<script id="tmp-latest-taps" type="text/template">
				&lt;% if (results.length) { %&gt;
					&lt;ul&gt;			
						&lt;% _.each(results, function(v) { %&gt;
							&lt;% if (v.attributes.venue.attributes.slug !== "demo") { %&gt;
							&lt;li&gt;
								&lt;img src="&lt;%=v.attributes.image%&gt;" /&gt;
								&lt;div class="left"&gt;
									&lt;h4&gt;&lt;a href="/brewery/&lt;%= v.attributes.breweryId %&gt;"&gt;&lt;%= v.attributes.breweryName %&gt;&lt;/a&gt;&lt;br /&gt;&lt;%= v.attributes.beerName %&gt;&lt;/h4&gt;
									&lt;p class="sub-info"&gt;&lt;%= v.attributes.abv &gt; 0 ? v.attributes.abv+"% " : "" %&gt;&lt;%= v.attributes.style %&gt;&lt;/p&gt;
									&lt;p class="time-ago"&gt;Tapped &lt;strong&gt;&lt;%= timeAgo(String(v.createdAt)) %&gt;&lt;/strong&gt;&lt;br /&gt;at &lt;a href="/&lt;%= v.attributes.venue.attributes.slug %&gt;"&gt;&lt;%= v.attributes.venue.attributes.name %&gt;&lt;/a&gt;&lt;/p&gt;
								&lt;/div&gt;
							&lt;/li&gt;
							&lt;% } %&gt;
						&lt;% }); %&gt;
					&lt;/ul&gt;
				&lt;% } else { %&gt;
					&lt;p style="text-align: center;"&gt;Official tap lists from partner venues coming soon!&lt;br /&gt;In the meantime search from over 120 craft beer venues to discover the latest reported beers.&lt;/p&gt;
				&lt;% } %&gt;

			</script>

			<script id="tmp-venue-header" type="text/template">
				&lt;h2&gt;&lt;%= name %&gt;&lt;/h2&gt;
				&lt;span class="byline"&gt;
					&lt;%=address == "" ? "" : address+", "%&gt;&lt;%=city == "" ? "" : city+", "%&gt;&lt;%=state%&gt;
				&lt;/span&gt;
			</script>

			<script id="tmp-venue-checkins" type="text/template">
					
				&lt;h3&gt;Latest Reported Beers*&lt;/h3&gt;
				&lt;ul&gt;
					&lt;% var beerArr = []; var count = 0; %&gt;
					&lt;% _.each(checkins.items, function(v, i) { %&gt;
						&lt;% var longAgo = timeAgo(v.created_at) %&gt;
						&lt;% if (_.indexOf(beerArr, v.beer.bid) == -1 &amp;&amp; count &lt; 20 &amp;&amp; longAgo.indexOf("months") == -1 &amp;&amp; longAgo.indexOf("year") == -1 &amp;&amp; longAgo.indexOf("years") == -1) { %&gt;
							&lt;% beerArr.push(v.beer.bid); count++; %&gt;
							&lt;li&gt;
								&lt;% if(v.beer.beer_label.indexOf('badge-beer-default') &lt; 0) { %&gt;
									&lt;img src="&lt;%=v.beer.beer_label%&gt;" /&gt;
								&lt;% } else if (v.brewery.brewery_label.indexOf('badge-brewery-default') &lt; 0) { %&gt;
									&lt;img src="&lt;%=v.brewery.brewery_label%&gt;" /&gt;
								&lt;% } else { %&gt;
									&lt;img src="images/default-label.png" /&gt;
								&lt;% } %&gt;
								&lt;div class="left"&gt;
									&lt;h4&gt;&lt;a href="/brewery/&lt;%= v.brewery.brewery_id %&gt;"&gt;&lt;%= v.brewery.brewery_name %&gt;&lt;/a&gt;&lt;br /&gt;&lt;%= v.beer.beer_name %&gt;&lt;/h4&gt;
									&lt;p class="sub-info"&gt;&lt;%= v.beer.beer_abv &gt; 0 ? v.beer.beer_abv+"% " : "" %&gt;&lt;%= v.beer.beer_style %&gt;&lt;/p&gt;
									&lt;p class="time-ago"&gt;Last reported &lt;strong&gt;&lt;%= longAgo %&gt;&lt;/strong&gt;&lt;/p&gt;
								&lt;/div&gt;
							&lt;/li&gt;
						&lt;% } %&gt;
					&lt;% }); %&gt;
				&lt;/ul&gt;
				&lt;% if (count == 0) { %&gt;
					&lt;p&gt;No recent reports found.&lt;/p&gt;
				&lt;% } else { %&gt;
					&lt;p&gt;* Reported beers provided by Untappd checkins. These maybe taps or bottles.&lt;/p&gt;
				&lt;% } %&gt;
			</script>

			<script id="tmp-venue-taps" type="text/template">

				&lt;h3&gt;Official Tap List&lt;/h3&gt;
				&lt;ul&gt;
					&lt;% _.each(results, function(v) { %&gt;

						&lt;li&gt;
							&lt;img src="&lt;%=v.attributes.image%&gt;" /&gt;

							&lt;div class="left"&gt;
								&lt;h4&gt;&lt;a href="/brewery/&lt;%= v.attributes.breweryId %&gt;"&gt;&lt;%= v.attributes.breweryName %&gt;&lt;/a&gt;&lt;br /&gt;&lt;%= v.attributes.beerName %&gt;&lt;/h4&gt;
								&lt;p class="sub-info"&gt;&lt;%= v.attributes.abv &gt; 0 ? v.attributes.abv+"% " : "" %&gt;&lt;%= v.attributes.style %&gt;&lt;/p&gt;
								&lt;p class="time-ago"&gt;Tapped &lt;strong&gt;&lt;%= timeAgo(String(v.createdAt)) %&gt;&lt;/strong&gt;&lt;/p&gt;
							&lt;/div&gt;
						&lt;/li&gt;

					&lt;% }); %&gt;
				&lt;/ul&gt;

			</script>

			<script id="tmp-brewery-header" type="text/template">
				&lt;h2&gt;&lt;%= brewery_name %&gt;&lt;/h2&gt;
				&lt;span class="byline"&gt;
					&lt;%=location.brewery_city ? location.brewery_city+"," : ""%&gt; &lt;%=location.brewery_state%&gt;
				&lt;/span&gt;
			</script>

			<script id="tmp-brewery-checkin" type="text/template">
				&lt;h3&gt;Latest Reported Venues*&lt;/h3&gt;
				&lt;ul&gt;
					&lt;% var beerArr = [] %&gt;
					&lt;% _.each(data, function(v) { %&gt;
						&lt;% var venueArr = [] %&gt;
						&lt;% if (v.venue  &amp;&amp; _.indexOf(beerArr, v.beer.bid) == -1) { %&gt;
							&lt;% beerArr.push(v.beer.bid) %&gt;
							&lt;li&gt;
								&lt;% if(v.beer.beer_label.indexOf('badge-beer-default') &lt; 0) { %&gt;
									&lt;img src="&lt;%=v.beer.beer_label%&gt;" /&gt;
								&lt;% } else if (v.brewery.brewery_label.indexOf('badge-brewery-default') &lt; 0) { %&gt;
									&lt;img src="&lt;%=v.brewery.brewery_label%&gt;" /&gt;
								&lt;% } else { %&gt;
									&lt;img src="images/default-label.png" /&gt;
								&lt;% } %&gt;
								&lt;div class="left"&gt;
									&lt;h4&gt;&lt;%= v.brewery.brewery_name %&gt;&lt;br /&gt;&lt;%= v.beer.beer_name %&gt;&lt;/h4&gt;
									&lt;p class="sub-info"&gt;&lt;%= v.beer.beer_abv &gt; 0 ? v.beer.beer_abv+"% " : "" %&gt;&lt;%= v.beer.beer_style %&gt;&lt;/p&gt;
									&lt;p class="time-ago"&gt;Last reported at&lt;/p&gt;
									&lt;% _.each(v.venue, function(x, i) { %&gt;
										&lt;% if (x != null) { %&gt;
											&lt;% var index = $.inArray(x.venue.venue_id, venues); %&gt;
											&lt;% if (!$.isArray(x.venue) &amp;&amp; _.indexOf(venueArr, x.venue.venue_id) == -1 &amp;&amp; index !== -1) { %&gt;
												&lt;% venueArr.push(x.venue.venue_id) %&gt;
												&lt;p class="time-ago"&gt;&lt;strong&gt;
													&lt;% if (x.venue.slug) { %&gt;&lt;a href="/&lt;%= x.venue.slug %&gt;"&gt;&lt;%= x.venue.venue_name %&gt;&lt;/a&gt;&lt;% } else { %&gt;&lt;%= x.venue.venue_name %&gt;&lt;% } %&gt;, &lt;%= x.venue.location.venue_state == '' ? '' : x.venue.location.venue_state+", " %&gt; &lt;%= timeAgo(x.created_at) %&gt;&lt;/strong&gt;&lt;/p&gt;
											&lt;% } %&gt;
										&lt;% } %&gt;
									&lt;% }); %&gt;
									&lt;/p&gt;
								&lt;/div&gt;
							&lt;/li&gt;
						&lt;% } %&gt;
					&lt;% }); %&gt;
				&lt;/ul&gt;
				&lt;p&gt;* Reported venues provided by Untappd checkins. These maybe taps or bottles.&lt;/p&gt;
			</script>

			<script id="tmp-brewery-taps" type="text/template">

				&lt;h3&gt;Officially tapped at&lt;/h3&gt;
				&lt;ul&gt;
					&lt;% _.each(results, function(v) { %&gt;
						&lt;li&gt;
							&lt;img src="&lt;%=v.beerLabel%&gt;" /&gt;
							&lt;div class="left"&gt;
								&lt;h4&gt;&lt;%= v.breweryName %&gt;&lt;br /&gt;&lt;%= v.beerName %&gt;&lt;/h4&gt;
